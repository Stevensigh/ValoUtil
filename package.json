{
  "name": "valo-util",
  "version": "1.0.0",
  "description": "Valo utility lineups",
  "bin": "dist/server/index.js",
  "scripts": {
    "clean": "rm -rf dist/",
    "build": "npm run build:client && npm run build:server && npm run build:shared",
    "build:client": "webpack",
    "build:client:watch": "npm run build:client -- --watch",
    "build:server": "babel src/server -d dist/server --copy-files --verbose",
    "build:server:watch": "npm run build:server -- --watch",
    "build:shared": "babel src/shared -d dist/shared --copy-files --verbose",
    "build:shared:watch": "npm run build:shared -- --watch",
    "start:client": "webpack-dev-server",
    "start:client:watch": "npm run start:client -- --watch",
    "start:server": "node -r @babel/register src/server",
    "start:server:watch": "nodemon -r @babel/register src/server -w src/server",
    "release": "pkg --targets node14 --output dist/bin/web-boilerplate .",
    "serve": "node dist/server",
    "dev": "concurrently --kill-others --prefix name --prefix-colors cyan.bold,magenta.bold --names HTTP,JS 'npm run start:server:watch' 'npm run build:client:watch'",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "cover": "npm run test -- --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Stevensigh/ValoUtil.git"
  },
  "author": "",
  "license": "UNLICENSED",
  "private": false,
  "pre-commit": [
    "lint",
    "test"
  ],
  "pkg": {
    "assets": [
      "dist/client/**/*"
    ]
  },
  "dependencies": {
    "@sentry/browser": "5.17.0",
    "@sentry/node": "5.17.0",
    "express": "4.17.1",
    "js-yaml": "3.14.0",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-elemental": "1.2.2",
    "react-elemental-fonts": "0.0.2",
    "react-favicon": "0.0.18",
    "react-redux": "7.2.0",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "read-pkg-up": "7.0.1",
    "recompose": "0.30.0",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0",
    "supercharged": "0.0.6",
    "winston": "3.2.1",
    "yargs": "15.3.1"
  },
  "devDependencies": {
    "@babel/cli": "7.10.3",
    "@babel/core": "7.10.3",
    "@babel/node": "7.10.3",
    "@babel/plugin-proposal-class-properties": "7.10.1",
    "@babel/plugin-proposal-decorators": "7.10.3",
    "@babel/preset-env": "7.10.3",
    "@babel/preset-react": "7.10.1",
    "@babel/register": "7.10.3",
    "@linkiwi/eslint-config": "2.1.1",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.0.1",
    "babel-loader": "8.1.0",
    "babel-plugin-module-resolver": "4.0.0",
    "babel-plugin-react-elemental": "1.2.3",
    "babel-plugin-transform-inline-environment-variables": "0.4.3",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "concurrently": "5.2.0",
    "copy-webpack-plugin": "6.0.2",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "7.2.0",
    "eslint-import-resolver-babel-module": "5.1.2",
    "eslint-plugin-import": "2.21.2",
    "eslint-plugin-jest": "23.13.2",
    "eslint-plugin-react": "7.20.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.0.1",
    "nodemon": "2.0.4",
    "pkg": "4.4.8",
    "pre-commit": "1.2.2",
    "raf": "3.4.1",
    "react-test-renderer": "16.13.1",
    "terser-webpack-plugin": "3.0.6",
    "url-loader": "4.1.0",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  }
}
